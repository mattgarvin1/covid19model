{
    "workflow": {
      "$graph": [
        {
          "baseCommand": [
            "bash",
            "/docker-run.sh"
          ],
          "class": "CommandLineTool",
          "cwlVersion": "v1.0",
          "id": "#main",
          "outputs": [
            {
              "id": "#main/standard_out",
              "outputBinding": {
                "glob": "stdout.txt"
              },
              "type": "File"
            }
          ],
          "requirements": [
            {
              "class": "DockerRequirement",
              "dockerPull": "quay.io/cdis/bayes-by-county:feat_cwl"
            },
            {
              "class": "ResourceRequirement",
              "coresMax": 4,
              "coresMin": 4,
              "ramMax": 16384,
              "ramMin": 16384
            }
          ],
          "stdout": "stdout.txt"
        }
      ],
      "cwlVersion": "v1.0"
    }
}